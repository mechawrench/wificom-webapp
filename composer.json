{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "clarkeash/doorman": "^8.0",
        "doctrine/dbal": "^3.5",
        "fakerphp/faker": "^1.21",
        "filament/filament": "^2.16",
        "filament/tables": "^2.16",
        "guzzlehttp/guzzle": "^7.2",
        "jeffgreco13/filament-breezy": "^1.5",
        "laravel/framework": "^10.15",
        "laravel/jetstream": "^2.15",
        "laravel/octane": "^1.4",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/vapor-core": "^2.27",
        "laravel/vapor-ui": "^1.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.11",
        "php-mqtt/laravel-client": "^1.2",
        "rakutentech/laravel-request-docs": "^1.31",
        "sentry/sentry-laravel": "^3.2",
        "spatie/laravel-ignition": "^2.0",
        "spiral/roadrunner": "^2.8.2",
        "yepsua/filament-themes": "^0.1.0"
    },
    "require-dev": {
        "knuckleswtf/scribe": "^4.16",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "pestphp/pest-plugin-livewire": "^1.0",
        "pestphp/pest-plugin-parallel": "^1.2",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "@php artisan vendor:publish --tag=filament-config"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
